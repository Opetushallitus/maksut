{
  "scripts": {
    "lint:clj": "clj-kondo --config oph-configuration/clj-kondo.config.edn --fail-level error --lint src",
    "lint:clj:lint-staged": "clj-kondo --config oph-configuration/clj-kondo.config.edn --lint"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-prepush"
    }
  },
  "lint-staged": {
    "src/**/*.{clj,cljs,cljc}": [
      "npm run lint:clj:lint-staged"
    ]
  },
  "lint-prepush": {
    "base": "master",
    "tasks": {
      "src/**/*.{clj,cljs,cljc}": [
        "npm run lint:clj:lint-staged"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "clj-kondo": "^2024.02.12",
    "husky": "^4.3.0",
    "lint-prepush": "^2.1.0",
    "lint-staged": "^10.5.0",
    "pm2": "^4.5.6",
    "shadow-cljs": "^2.27.4",
    "webpack": "^5.3.0"
  },
  "dependencies": {
    "create-react-class": "^15.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
