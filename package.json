{
  "scripts": {
    "lint:clj": "clj-kondo --config oph-configuration/clj-kondo.config.edn --fail-level error --lint src",
    "lint:clj:lint-staged": "clj-kondo --config oph-configuration/clj-kondo.config.edn --lint"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-prepush"
    }
  },
  "lint-staged": {
    "src/**/*.{clj,cljs,cljc}": [
      "npm run lint:clj:lint-staged"
    ]
  },
  "lint-prepush": {
    "base": "master",
    "tasks": {
      "src/**/*.{clj,cljs,cljc}": [
        "npm run lint:clj:lint-staged"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/mocha": "^8.0.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "clj-kondo": "^2024.02.12",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-prepush": "^2.1.0",
    "lint-staged": "^10.5.0",
    "pg": "^8.6.0",
    "pm2": "^4.5.6",
    "prettier": "^2.1.2",
    "shadow-cljs": "^2.27.4",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "uuid": "^8.3.2",
    "webpack": "^5.3.0"
  },
  "dependencies": {
    "create-react-class": "^15.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^4.3.0"
  }
}
